#appbar
	#appbar--brand(onclick='openDrawer()') &nbsp; ☰ &nbsp;
		span.mui--hidden-xs Topseed&trade; Hello
	ul#appbar--menu
		li
			a(href='/linkBlog/list/') List
		li
			a(href='/linkBlog/admin/') Admin

#sidedrawer(onclick='closeDrawer()')
	#sidedrawer--brand &nbsp; ☰ Topseed&trade; Hello
	.mui-divider
	ul#sidedrawer--menu
		li
			a(href='/linkBlog/list/') List
		li
			a(href='/linkBlog/login/') Login
		li
			a(href='/linkBlog/admin/') Admin

script.
	function openDrawer(px, e) {
		$('#sidedrawer').css('transform', 'translateX(201px)')
	}
	function closeDrawer(e) {
		$('#sidedrawer').css('transform', 'translateX(0px)')
	}
